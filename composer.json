{
    "name": "idanieldrew/redact",
    "type": "project",
    "description": "Redact is a place to write article.",
    "keywords": [
        "laravel",
        "article",
        "redact",
        "php",
        "project"
    ],
    "authors": [
        {
            "name": "Daniel",
            "email": "danielgia35@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0||^8.1",
        "ext-json": "*",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jeroen-g/explorer": "^3.2",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.10",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.6",
        "laravel/tinker": "^2.7",
        "morilog/jalali": "3.*",
        "php-amqplib/php-amqplib": "^3.4.0",
        "predis/predis": "^2.0.2",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "spatie/laravel-ignition": "^1.0",
        "laravel/telescope": "^4.9",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "files": [
            "Modules/Share/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Module\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
